<p id="notice"><%= notice %></p>

<h1>Jfs</h1>

<table>
  <thead>
    <tr>
      <th>Question Number</th>

      <th>Heading</th>
      
      <th>Point</th>
      <th>Creaated At</th>
      <th>Updated At</th>

     
      <th colspan="8"></th>
    </tr>
  </thead>

  <tbody>
    <% @jfs.each do |jf| %>
      <tr>
        <td><%= jf.id %></td>
       
        <td><%= jf.heading %></td>
        
        <td><%= jf.point %></td>
        <td><%= jf.created_at %></td>
        <td><%= jf.updated_at %></td>
       
        <td><%= link_to 'Click To Shot', jf %></td>
        <td><%= link_to 'Edit', edit_jf_path(jf) %></td>
        <td><%= link_to 'Destroy', jf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jf', new_jf_path %>
