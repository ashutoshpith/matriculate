<p id="notice"><%= notice %></p>
<div class="centred">
<h1 >Java Looping</h1>
<div class="loop-topic"> 
<table >
  <thead>
    <tr >
      <th >Question Number</th>
     
      <th >Heading</th>
      
      <th>Point</th>
      <th>Created At</th>
      <th>Updated At</th>
      
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @jls.each do |jl| %>
      <tr>
        <td><%= jl.id %></td>
      
        <td><%= jl.heading %></td>
        
        <td><%= jl.point %></td>
        <td><%= jl.created_at %></td>
        <td><%= jl.updated_at %></td>
        
       
        <td><%= link_to 'Clcik To Shot', jl %></td>
        <td><%= link_to 'Edit', edit_jl_path(jl) %></td>
        <td><%= link_to 'Destroy', jl, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>
<ul>
<strong>
<li>
<%= link_to 'New Jl', new_jl_path %>
</li><li>
<%= link_to  "Back", java_practice_path %>
</li>
</strong>
</ul>