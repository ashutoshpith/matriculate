<p id="notice"><%= notice %></p>

<h1>Java Practice Arena</h1>

<table>
  <thead>
    <tr>
      <th>Question Number:</th>

      <th>Heading</th>
     
      <th>Point</th>
      <th>Created At</th>
      <th>Updated At</th>
      
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @jps.each do |jp| %>
      <tr>
        <td><%= jp.id %></td>
        
        <td><%= jp.heading %></td>
        
        <td><%= jp.point %></td>
        <td><%= jp.created_at %></td>
        <td><%= jp.updated_at %></td>

       
        
        <td><%= link_to 'Click To Shot', jp %></td>
        <td><%= link_to 'Edit', edit_jp_path(jp) %></td>
        <td><%= link_to 'Destroy', jp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<hr>
<br>
<strong>
<%= link_to 'New Jp', new_jp_path %>
<br>
<br>
<%= link_to  "Back To Java Practice Arena", java_practice_path %></strong>