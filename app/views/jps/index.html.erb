<p id="notice"><%= notice %></p>


<div class="container centre">
<h1>Java Practice Arena</h1>
<br>
<table class="table table-striped table-hover">
  <thead class="thead-dark">
    <tr>
      <th>Question No:</th>

      <th>Heading:</th>
     
      <th>Point</th>
      <th>Created At</th>
      <th>Updated At</th>
      
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @jps.each do |jp| %>
      <tr>
        <td><%= jp.id %></td>
        
        <td><%= jp.heading %></td>
        
        <td><%= jp.point %></td>
        <td><%= jp.created_at %></td>
        <td><%= jp.updated_at %></td>

       
        
        <td><%= link_to 'Click To Shot', jp %></td>
        <td><%= link_to 'Edit', edit_jp_path(jp) %></td>
        <td><%= link_to 'Destroy', jp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>
<strong class="centre">
<%= link_to 'Create New Content', new_jp_path, class: "btn btn-outline-info" %>
<br>
<br>
<%= link_to  "Back To Java Practice Arena", java_practice_path, class: "btn btn-outline-info" %></strong>